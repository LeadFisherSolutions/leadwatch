{
  "root": true,
  "extends": [
    "eslint:recommended",
    "leadfisher",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "strict": "off",
    "import/no-unresolved": "off",
    "class-methods-use-this": "off"
  }
}
